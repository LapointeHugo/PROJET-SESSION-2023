@using ProjetSessionHL.Models;
@model PageRechercheViewModel;

@foreach (var item in @Model.Resultat)
{
    <div class="col-xl-3 col-lg-3 col-md-6 col-sm-6 col-xs-6 result">
        <img src="/images/@(item.ImgFile)" class="imageLogo">
        <p class="titreEquipe">@(item.Nom)</p>
    </div>
}